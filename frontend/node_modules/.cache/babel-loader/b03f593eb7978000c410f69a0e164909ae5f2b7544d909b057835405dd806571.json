{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport authWrapper from \"../../components/auth-wrapper.vue\";\nimport apiService from \"../../services/api.services\";\nexport default {\n  name: \"login.vue\",\n  components: {\n    \"auth-wrapper\": authWrapper\n  },\n  methods: {\n    newlogin() {\n      this.$router.push(\"/auth/register\");\n    },\n    onForgotPasswordClick() {\n      this.$router.push(\"/auth/forgot-password\");\n    },\n    loginButton() {\n      this.isloading = true;\n      const data = {\n        username: this.username,\n        password: this.password\n      };\n      //if both user name and passowrd then login user\n      if (this.username && this.password) {\n        apiService.login(data).then(() => {\n          this.isloading = false;\n          this.showError = false;\n          this.errorMessage = \"\";\n          this.$router.push(\"/main\");\n        }).catch(error => {\n          this.isloading = false;\n          console.log(error);\n          this.showError = true;\n          this.errorMessage = error && error.response && error.response.data && error.response.data.message;\n        });\n      }\n    }\n  },\n  data() {\n    return {\n      username: \"\",\n      password: \"\",\n      showError: false,\n      errorMessage: \"\",\n      isloading: false\n    };\n  }\n};","map":{"version":3,"mappings":";AAkDE,OAAOA,WAAU,MAAO,mCAAmC;AAC3D,OAAOC,UAAS,MAAO,6BAA6B;AACpD,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACV,cAAc,EAAEH;EAClB,CAAC;EACDI,OAAO,EAAE;IACPC,QAAQ,GAAG;MACT,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,gBAAgB,CAAC;IACrC,CAAC;IACDC,qBAAqB,GAAE;MACtB,IAAI,CAACF,OAAO,CAACC,IAAI,CAAC,uBAAuB,CAAC;IAC3C,CAAC;IACDE,WAAW,GAAG;MACZ,IAAI,CAACC,SAAS,GAAC,IAAG;MAClB,MAAMC,IAAG,GAAI;QACXC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC;MACD;MACA,IAAI,IAAI,CAACD,QAAO,IAAK,IAAI,CAACC,QAAQ,EAAE;QAClCZ,UAAS,CACNa,KAAK,CAACH,IAAI,EACVI,IAAI,CAAC,MAAM;UACV,IAAI,CAACL,SAAS,GAAC,KAAI;UACnB,IAAI,CAACM,SAAQ,GAAI,KAAK;